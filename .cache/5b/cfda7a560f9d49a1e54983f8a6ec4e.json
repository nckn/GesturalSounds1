ch(element => {\n    element.sound = loadSound(element.path, soundIsLoaded)\n  });\n  slider = createSlider(0, 1, 0, 0.01)\n  \n\n  flippedVideo = ml5.flipImage(video)\n  // Start classifying\n  classifyVideo()\n}\n\nfunction soundIsLoaded () {\n  sounds.forEach(element => {\n    element.sound.play()\n  })\n}\n\nfunction draw () {\n  background(0)\n  // Draw the video\n  image(flippedVideo, 0, 0)\n\n  // Set volume\n  // song.setVolume(slider.value());\n  \n  // Draw the label\n  fill(255, 0, 0)\n  textSize(16)\n  textAlign(CENTER)\n  text(label, width / 2, height - 4)\n}\n\n// Get a prediction for the current video frame\nfunction classifyVideo () {\n  flippedVideo = ml5.flipImage(video)\n  classifier.classify(flippedVideo, gotResult)\n}\n\n// When we get a result\nfunction gotResult (error, results) {\n  // If there is an error\n  if (error) {\n    console.error(error)\n    return\n  }\n  // The results are in an array ordered by confidence.\n  // console.log(results[0]);\n  label = results[0].label\n\n  if (results[0].label == 'Class 2' && sounds[0].canPlay) { \n    print('cool burger')\n    sounds[0].sound.play()\n    sounds[0].canPlay = false\n    sounds[1].canPlay = true\n  } else if (results[0].label == 'Class 3' && sounds[1].canPlay) {\n    sounds[1].sound.play()\n    sounds[1].canPlay = false\n    sounds[0].canPlay = true\n  }\n  \n  // Classifiy again!\n  classifyVideo()\n}\n"},"lineCount":null}},"error":null,"hash":"ec2492bc178fce3907ca8b308af54c44","cacheData":{"env":{}}}